<div id='wrap'>
  <script>
    Include('/hbs/layout/Header_1-a.hbs');
  </script>
  <div class='wrap'>
    <div class='content flex justify-between'>
      <script>Include('/hbs/layout/Lnb_1-a.hbs');</script>
      <div class='content-wrap'>
        <div class='content-inner text-xs shadow-lg'>
          <script>Include('/hbs/component/TitleBox_1-a.hbs');</script>
          <div class='p-7'>
            <!-- 검색 조건 -->
            <div class='tbl-box'>
              <div class='tit-box'><h3>검색 조건</h3></div>
              <table class='table' >

                <colgroup>
                  <col style='width:15%' />
                  <col style='width:55%' />
                </colgroup>
                <tbody>
                  <tr>
                    <th scope='row'>검색어</th>
                    <td>
                      <input type='text' class='input-wrap' placeholder='placeholder' />
                    </td>
                  </tr>
                  <tr>
                    <th scope='row'>사용 유무</th>
                    <td>
                      <label class='label mr-5'>
                        <input type='radio' name='board_use' checked='' />
                        <span class='label-text'>전체</span>
                      </label>
                      <label class='label mr-5'>
                        <input type='radio' name='board_use' />
                        <span class='label-text'>사용</span>
                      </label>
                      <label class='label'>
                        <input type='radio' name='board_use' />
                        <span class='label-text'>미사용</span>
                      </label>
                    </td>
                  </tr>
                  <tr>
                    <th scope='row'>게시판 구분</th>
                    <td>
                      <label class='label mr-5'>
                        <input type='radio' name='board_classify' checked='' />
                        <span class='label-text'>전체</span>
                      </label>
                      <label class='label mr-5'>
                        <input type='radio' name='board_classify' />
                        <span class='label-text'>기본</span>
                      </label>
                      <label class='label'>
                        <input type='radio' name='board_classify' />
                        <span class='label-text'>추가</span>
                      </label>
                    </td>
                  </tr>
                  <tr>
                    <th scope='row'>게시판 유형</th>
                    <td>
                      <label class='label mr-5'>
                        <input type='radio' name='board_type' checked='' />
                        <span class='label-text'>전체</span>
                      </label>
                      <label class='label mr-5'>
                        <input type='radio' name='board_type' />
                        <span class='label-text'>일반</span>
                      </label>
                      <label class='label mr-5'>
                        <input type='radio' name='board_type' />
                        <span class='label-text'>갤러리</span>
                      </label>
                      <label class='label mr-5'>
                        <input type='radio' name='board_type' />
                        <span class='label-text'>문의</span>
                      </label>
                      <label class='label'>
                        <input type='radio' name='board_type' />
                        <span class='label-text'>후기</span>
                      </label>
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
            <div class='text-center mt-6'>
              <button type='button' class='btn-m'>검색</button>
              <button type='button' class='btn-white-m'>초기화</button>
            </div>
            <!-- //검색 조건 -->
            <!-- 게시판 리스트 -->
            <div>
              <div class='tit-box mt-6'><h3>게시판 리스트</h3></div>
              <div id='app'></div>
              <div class='mt-3'>
                <span class='inline-block mr-1'>* 선택 게시판</span>
                <button type='button' class='btn-warning-m'>삭제</button>
              </div>
            </div>
            <!-- //게시판 리스트 -->
          </div>
        </div>
      </div>
    </div>
  </div>
  <script>
    Include('/hbs/layout/Footer_1-a.hbs');
  </script>
</div>